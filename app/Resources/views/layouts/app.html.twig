        <title>{% block title %}PréDoc{% endblock %}</title>
        
        {# lien d'integration de Style Boostrap #}
        <link rel="stylesheet" type="text/css" href="https://bootswatch.com/cosmo/bootstrap.min.css">
        {# lien d'integration de icons #}

        <link rel="stylesheet" type="text/css" href="http://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css">
        <link rel="stylesheet" type="text/css" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        {# lien de style generale de template  #}
        <link rel="stylesheet" type="text/css" href="{{asset('bundles/app/css/style.css')}}">
        {% block stylesheets %}{% endblock %} 
    </head>
    <body id="app"> 

    {# include de navbar#}

    {{ include('layouts/navbar.html.twig') }} 
    <div class="content-app">
